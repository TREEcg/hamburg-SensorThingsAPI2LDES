@prefix js: <https://w3id.org/conn/js#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.

js:FetchStapi a js:JsProcess;
    js:file <./lib/index.js>;
    js:function "fetchStapi";
    js:location <./>;
    js:mapping [
        a fno:Mapping;
        fno:parameterMapping [
            a fnom:PositionParameterMapping ;
            fnom:functionParameter "url";
            fnom:implementationParameterPosition "0"^^xsd:integer
        ], [
           a fnom:PositionParameterMapping ;
           fnom:functionParameter "writer";
           fnom:implementationParameterPosition "1"^^xsd:integer
        ], [
            a fnom:PositionParameterMapping ;
            fnom:functionParameter "pageSize";
            fnom:implementationParameterPosition "2"^^xsd:integer
        ]
    ].

[] a sh:NodeShape;
    sh:targetClass js:FetchStapi;
    sh:property [
        sh:path js:url;
        sh:name "url";
        sh:datatype xsd:string;
        sh:maxCount 1;
        sh:minCount 1;
    ], [
        sh:path js:writer;
        sh:name "writer";
        sh:class :WriterChannel;
        sh:maxCount 1;
        sh:minCount 1
    ], [
        sh:path js:pageSize;
        sh:name "pageSize";
        sh:datatype xsd:integer;
        sh:maxCount 1;
        sh:minCount 1;
    ].